{
  "name": "emrc-watcher",
  "version": "0.0.1",
  "description": "watch file",
  "main": "build/index",
  "dependencies": {
    "chokidar": "^1.6.1",
    "ramda": "^0.23.0",
    "ramda-fantasy": "^0.7.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.0",
    "nyc": "^10.0.0",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.6"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015",
      "stage-2"
    ],
    "sourceMaps": "inline",
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": "5",
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "lines": 5,
    "statements": 5,
    "functions": 5,
    "branches": 5,
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "check-coverage": true,
    "all": true,
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "lint": "eslint ./src/**",
    "test": "npm run lint && cross-env NODE_ENV=test nyc ava",
    "coverage": "nyc report --reporter=html"
  },
  "author": "",
  "license": "ISC"
}
